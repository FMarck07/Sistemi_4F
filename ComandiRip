!RT-1
enable 
configure terminal 
hostname RT-1
router rip
version 2
network 10.0.0.0
exit
!- Fast Ethernet 0/0 RT-1 -> RT-2
interface fastethernet 0/0
bandwidth 501
no shutdown
!- Fast Ethernet 1/0 RT-1 -> RT-3
interface fastethernet 1/0
bandwidth 725
no shutdown
end
exit

!RT-2
